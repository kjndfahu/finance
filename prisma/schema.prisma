generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())

  login       String
  name        String
  surname     String
  email       String
  telegramId  String
  phoneNumber Int
  region      Int
  password    String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
